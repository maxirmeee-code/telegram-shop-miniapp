{
  "version": 2,
  "builds": [
    {
      "src": "bot/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "web/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/webhook", "dest": "bot/index.js" },
    { "src": "/", "dest": "/web/index.html" },
    { "src": "/(.)", "dest": "/web/$1" }
  ]
}